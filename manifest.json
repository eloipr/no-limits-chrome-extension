{
  "name": "No AdBlock limits",
  "version": "1.0",
  "description": "Hides blocking popups telling to disable AdBlock.",
  "permissions": ["activeTab"],
  "page_action": {
    "default_popup": "popup.html",
    "default_icon": {
      "16": "images/unlock-icon.png",
      "32": "images/unlock-icon.png",
      "48": "images/unlock-icon.png",
      "128": "images/unlock-icon.png"
    }
  },
  "icons": {
    "16": "images/unlock-icon.png",
    "32": "images/unlock-icon.png",
    "48": "images/unlock-icon.png",
    "128": "images/unlock-icon.png"
  },
  "manifest_version": 2
}
